# simple sorting
arr=[1,2,4,3,6,5,7]
length = len(arr)
j = 0
while j < length - 1:
    if (arr[j] > arr[j + 1]):
        temp = arr[j]
        arr[j] = arr[j + 1]
        arr[j + 1] = temp
        j = -1
    j += 1
print(arr)


##
a=[9,9]
b=0
i=0
c=0
l=[]
if len(a)==1:
    c=a[0]+1
    l.append(c)
    print(c)
else:
    while len(a)-1!=0 and i<=len(a)-1:
        b=(b*10)+a[i]
        i+=1
    b=b+1
    print(b)
    
